{
  "root": true,
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "prettier/flowtype"
  ],
  "globals": {
    "Generator": true
  },
  "plugins": [
    "jsx-a11y",
    "flowtype"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-restricted-syntax": 1,
    "no-shadow": 1,
    "no-nested-ternary": 1,
    "no-underscore-dangle": 1,
    "arrow-body-style": 0,
    "react/prop-types": 1,
    "react/no-unused-prop-types": 1,
    "react/forbid-prop-types": 1,
    "react/prefer-stateless-function": 1,
    "react/require-default-props": 1,
    "react/jsx-filename-extension": 0
  }
}